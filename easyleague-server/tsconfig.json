{
  "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "pretty": true,
      "sourceMap": true,
      "target": "es6",
      "outDir": "./dist",
      "baseUrl": "./lib",
      "experimentalDecorators": true,
      "allowJs": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.js",
    "server.ts"
  ],
  "exclude": [
      "node_modules"
  ]
}
